<%= simple_form_for(@ref) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :version %>
    <%= f.input :channel %>
    <%= f.input :exec_file, label: '文件' %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "提交", class: 'btn btn-info'%> <%= link_to '返回', refs_path, class: 'btn btn-default' %>

  </div>
<% end %>
